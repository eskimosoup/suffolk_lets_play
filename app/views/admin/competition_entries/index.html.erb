<% title CompetitionEntry.model_name.human.pluralize %>

<% unless params[:competition_id].blank? %>
  <div class="admin_functions">
    <%= link_to 'Back', admin_competitions_path %>
  </div>
<% end %>

<%= informant_table @competition_entries, {
      :headings => [ 'Name', 'Postcode', 'Phone', 'Email', '' ], 
      :body => [ :name, :postcode, :phone, :email, it_edit ]
    } %>
